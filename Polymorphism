package hi;
class Vehicle{
	void start() {
		System.out.println("The Vehicle class is starting");			}
}
class car extends Vehicle {
	void start() {
		System.out.println("The car class is starting ");
	}
}
class bike extends Vehicle{
	void start() {
		System.out.println("The bike class is starting");
	}
	void type() {
		System.out.println("Yahama bike");
	}
}
public class Polymorphism {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Vehicle v;
		v=new Vehicle();
		v.start();
		v=new car();
		v.start();
		v=new bike();
		v.start();
		bike b;
		b=new bike();
		b.type();
		
		

	}

}
